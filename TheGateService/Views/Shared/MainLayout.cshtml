@using System
@using System.Collections
@using System.Collections.Generic
@using System.Linq
<!DOCTYPE html>
<html lang="en">
    @Html.Partial("HtmlHeader")
    <body>
        <div class="container">
            <div class="row">
                @Html.Partial("Header")
                <div id="sidebar" class="span2">
                    @Html.Partial("Sidebar")
                </div>
                <div id="body" class="span10">
                    <div id="content">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
        @Html.Partial("Footer")
    </body>
</html>